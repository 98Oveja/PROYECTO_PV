               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <VBox alignment="BOTTOM_LEFT" prefHeight="222.0" prefWidth="406.0">
               <children>
                  <Label fx:id="Descripcion" prefHeight="114.0" prefWidth="348.0" style="-fx-background-color: rgba(0,0,0,.7);" text="Descripcion" textFill="WHITE" wrapText="true">
